{
  "name": "realtube-extension",
  "version": "0.1.0",
  "description": "Community-powered AI content detection for YouTube",
  "private": true,
  "scripts": {
    "build": "npm run build:chrome && npm run build:firefox",
    "build:chrome": "webpack --config webpack.config.js --env browser=chrome",
    "build:firefox": "webpack --config webpack.config.js --env browser=firefox",
    "dev": "webpack --config webpack.config.js --env browser=chrome --watch",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/chrome": "^0.0.287",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "copy-webpack-plugin": "^12.0.0",
    "css-loader": "^7.1.0",
    "fake-indexeddb": "^6.2.5",
    "jsdom": "^28.0.0",
    "mini-css-extract-plugin": "^2.9.0",
    "rimraf": "^6.0.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18",
    "webpack": "^5.97.0",
    "webpack-cli": "^6.0.0"
  },
  "dependencies": {
    "react": "^19.0.1",
    "react-dom": "^19.0.1"
  }
}
